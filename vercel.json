{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "framework": null,
    "env": {
        "PYTHON_BACKEND_URL": "https://prajwalk12-reqgen-api.hf.space"
    },
    "functions": {
        "api/**/*.ts": {
            "memory": 1024,
            "maxDuration": 60
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}